<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br />
  <%= f.collection_select :genre_id, Genre.all, :id, :name %><br>

  <%= f.text_field :notes, id:"song_notes_attributes_0_content", name:"song[song_notes][]" %><br>
  <%= f.text_field :notes, id:"song_notes_attributes_1_content", name:"song[song_notes][]" %>

  <%= f.submit %>
<% end %>


