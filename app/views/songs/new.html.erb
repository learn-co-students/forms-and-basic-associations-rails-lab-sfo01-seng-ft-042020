<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />

  <%= f.label :artist_name, :artist %>
  <%= f.text_field :artist_name %>
  <br />

  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
  <br />
  
  <input name="song[notes][]" id="song_notes_attributes_0_content">
  <input name="song[notes][]" id="song_notes_attributes_1_content">
  <%= f.submit %>
<% end %>


